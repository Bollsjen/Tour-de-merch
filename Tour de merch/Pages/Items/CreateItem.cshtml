@page
@model Tour_de_merch.Pages.Items.CreateItemModel
@{
}

<h1>Create Items</h1>

<form method="post">
    <div class="form-group">
        <label asp-for="@Model.Item.Titel" class="control-label "> <b class="">Titel</b> </label>
        <input asp-for="@Model.Item.Titel" class="form-control" />
       
    </div>
    <div class="form-group">
        <label asp-for="@Model.Item.Størrelse" class="control-label "> <b class="">Størrelse</b> </label>
        <input asp-for="@Model.Item.Størrelse" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="@Model.Item.Dato" class="control-label "> <b class="">Dato</b> </label>
        <input asp-for="@Model.Item.Dato" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="@Model.Item.Pris" class="control-label "> <b class="">Pris</b> </label>
        <input asp-for="@Model.Item.Pris" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="@Model.Item.PåLager" class="control-label "> <b class="">På Lager</b> </label>
        <input asp-for="@Model.Item.PåLager" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>
</form>
